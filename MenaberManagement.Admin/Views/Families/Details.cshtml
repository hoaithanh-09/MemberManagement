@model MemberManagerment.ViewModels.FamilyViewModels.FamilyVM

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid px-4">
    <h1 class="mt-4">Chi tiết</h1>
    <dl class="row">
        <dt class="col-sm-2">
            Số tứ tự
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt class="col-sm-2">
            Đại diện hộ gia đình
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HousldRepre)
        </dd>
        <dt class="col-sm-2">
            Năm sinh
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.YearBirth)
        </dd>
        <dt class="col-sm-2">
            Số lượng hội viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MumberMembers)
        </dd>
        <dt class="col-sm-2">
            Mã hội viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdMember)
        </dd>
        <dt class="col-sm-2">
            Số điện thoại
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
    </dl>

    <div>
        <a asp-action="Edit" asp-route-id="@Model.Id">Sửa</a> |
        <a asp-action="Index">Trở về</a>
    </div>
</div>